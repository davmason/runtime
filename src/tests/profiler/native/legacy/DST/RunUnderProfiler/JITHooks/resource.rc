#include "resource.h"

STRINGTABLE
BEGIN
    IDS_ProfilerCallbackDescription "JITHooks Profiler Test"
END

JITHooks         REGISTRY "JITHooks.rgs"

/*********************************************************************************
 *
 * Following part contains just version information for JITHooks.dll
 *
 *********************************************************************************/

#define VER_TESTLOCATION "profiler/tests/RunUnderProfiler/JITHooks"

#include <winver.h>
//#include <version/version.h>
#include "CLRTestVersion.h"

#define VER_PRODUCTVERSION 1,0,0,0
#define VER_PRODUCTVERSION_STR "" ## VER_PRODUCTVERSION ## ""
#define VER_FILEVERSION        rmj, rmm, rup, rpt
#define VER_FILEOS        VOS__WINDOWS32
#define VER_COMPANYNAME_STR    "Microsoft Corporation\0"
#define VER_FILEVERSION_STR                 rmj "." \
                                            rmm "." \
                                            rup "." \
                                            rpt "\0"

#ifdef _DEBUG
#define VER_FILEFLAGS        VS_FF_DEBUG
#else
#define VER_FILEFLAGS        VS_FF_SPECIALBUILD
#endif

#define VER_FILEDESCRIPTION_STR "CLR .NET Developer Services Tests.\0"

VS_VERSION_INFO     VERSIONINFO
  FILEVERSION       VER_FILEVERSION
  PRODUCTVERSION    VER_PRODUCTVERSION
  FILEFLAGSMASK     VER_FILEFLAGS
  FILEFLAGS         VER_FILEFLAGS
  FILEOS            VER_FILEOS
  FILETYPE          VFT_DLL
BEGIN
    BLOCK "VarFileInfo" 
    BEGIN 
        VALUE "Translation", 0x409, 1252
            // English language (0x409) and the Windows ANSI codepage (1252)
    END
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4" // Lang=US English, CharSet=Windows Multilingual
        BEGIN 
          // Note:  Non-standard value strings must be first as FileManager has a bug
          // that leaves garbage in its name buffer otherwise.
#ifdef VER_TESTLOCATION
          VALUE "Test location",   VER_TESTLOCATION "\0"
#endif

          VALUE "CompanyName",     VER_COMPANYNAME_STR
          VALUE "FileDescription", VER_FILEDESCRIPTION_STR
          VALUE "FileVersion",     VER_FILEVERSION_STR

          VALUE "InternalName",    "JITHooks"
          VALUE "LegalCopyright",  "Copyright MS()"
          VALUE "LegalTrademarks", "Trademarks"
          VALUE "OriginalFilename","OOPP"
          VALUE "ProductName",     "Managed Profiler"
          VALUE "ProductVersion",  VER_PRODUCTVERSION_STR
          VALUE "Comments",        VER_FILEDESCRIPTION_STR
        END
    END
END

